<%- include ('../../partials/header.ejs'); -%>
<%- include ('../../partials/navbar.ejs'); -%>
  
<section class="container">
    <div class="row my-4">
        <div class="col-md-12">
            <h1></i> Create new User</h1>
            <div><a href="/admin/users/create" class="btn btn-primary"><i class="fas fa-user"></i> New User</a></div>
            <hr>            
        </div>
    </div>
    <div class="row my-4">
        <div class="col-md-12">
            <table class="table table-hover table-striped bordered-table">
              <thead>
                <tr>
                  <th scope="col">ID</th>
                  <th scope="col">Email</th>
                  <th scope="col"></th>
                </tr>
              </thead>
              <tbody>
                <% users.forEach((users) => { %>                   
                  <tr>
                  <th scope="row"><%= users.id %></th>
                  <td><%= users.email %></td>
                  <td>
                     <div class="d-flex justify-content-end">                        

                     </div>
                  </td>    
                  </tr>
                <% }) %>   
              </tbody>
            </table>
            
        </div>
    </div>
    
</section>   

<%- include ('../../partials/footer.ejs'); -%>