<%- include ('partials/header.ejs'); -%>
<%- include ('partials/homenavbar.ejs'); -%>
<section class="container">
    <div class="row">
        <div class="col-md-9">
          
              <% articles.forEach((article) => { %>                   
              
                <div class="card my-4">
                  <div class="card-header">
                   <h2><%= article.title %></h2>
                  </div>
                  <div class="card-body">
                    <p class="card-text"> <%- article.body %> </p>
                  </div>
                </div>
              <% }) %>          
        </div>
        <div class="col-md-3">
          
        </div>
    </div>
</section>   
  

<%- include ('partials/footer.ejs'); -%>
